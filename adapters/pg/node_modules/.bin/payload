#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/juandominguez/development/payloadcms-vectorize/node_modules/.pnpm/payload@3.69.0_graphql@16.12.0_typescript@5.7.3/node_modules/payload/node_modules:/Users/juandominguez/development/payloadcms-vectorize/node_modules/.pnpm/payload@3.69.0_graphql@16.12.0_typescript@5.7.3/node_modules:/Users/juandominguez/development/payloadcms-vectorize/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/juandominguez/development/payloadcms-vectorize/node_modules/.pnpm/payload@3.69.0_graphql@16.12.0_typescript@5.7.3/node_modules/payload/node_modules:/Users/juandominguez/development/payloadcms-vectorize/node_modules/.pnpm/payload@3.69.0_graphql@16.12.0_typescript@5.7.3/node_modules:/Users/juandominguez/development/payloadcms-vectorize/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../payload/bin.js" "$@"
else
  exec node  "$basedir/../payload/bin.js" "$@"
fi
